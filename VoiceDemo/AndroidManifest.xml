<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.voicedemo"
    android:versionCode="1"
    android:versionName="1.0" >
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/mobile"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.voicedemo.WelcomeScreenActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.example.voicedemo.MainActivity"
            android:label="@string/app_name"
        android:parentActivityName="com.example.voicedemo.WelcomeScreenActivity" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.voicedemo.WelcomeScreenActivity" />
        </activity>
        
        <activity
        android:name="com.example.voicedemo.InstallActivity"
        android:label="@string/app_name"
        android:parentActivityName="com.example.voicedemo.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.voicedemo.MainActivity" />
    </activity>
    
        <activity
        android:name="com.example.voicedemo.RepairActivity"
        android:label="@string/app_name"
        android:parentActivityName="com.example.voicedemo.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.voicedemo.MainActivity" />
    </activity>
    
        <activity
        android:name="com.example.voicedemo.AssembleActivity"
        android:label="@string/app_name"
        android:parentActivityName="com.example.voicedemo.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.voicedemo.MainActivity" />
    </activity>
    
        <activity
        android:name="com.example.voicedemo.OperateActivity"
        android:label="@string/app_name"
        android:parentActivityName="com.example.voicedemo.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.voicedemo.MainActivity" />
    </activity>
    
        <activity
        android:name="com.example.voicedemo.HelpActivity"
        android:label="@string/app_name"
        android:parentActivityName="com.example.voicedemo.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.voicedemo.MainActivity" />
    </activity>
   
        <activity
        android:name="com.example.voicedemo.VideoActivity"
        android:label="@string/app_name"
        android:parentActivityName="com.example.voicedemo.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.voicedemo.MainActivity" />
    </activity>
    
    </application>

</manifest>
